<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data> 
        <record id="paperformat_custom" model="report.paperformat">
            <field name="name">Formato de papel</field>
            <field name="default" eval="True"></field>
            <field name="format">custom</field>
            <field name="page_height">210</field>
            <field name="page_width">80</field>
            <field name="orientation">Portrait</field>
            <!--field name="margin_top">10</field-->
            <field name="margin_bottom">10</field>
            <field name="margin_left">10</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"></field>
            <!--field name="header_spacing">5</field-->
            <field name="dpi">100</field>
        </record>

        <record id="report_cartaporte_r" model="ir.actions.report">
            <field name="name">Carta Porte</field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">cartaportegser.report_carta_porte</field>
            <field name="report_file">cartaportegser.report_carta_porte</field>
            <field name="paperformat_id" ref="cartaportegser.paperformat_custom"></field>
        </record>

    </data>
</odoo>