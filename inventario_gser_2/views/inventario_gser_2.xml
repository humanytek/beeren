<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_picking_form_inherit" model="ir.ui.view">
            <field name="name">stock.picking.form.inherit</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_stock_move_line_form"/>
            <field name="arch" type="xml">
                <page>
                    <field name="//field[@name='product_id']" position="after">
                    <field optional="show" name="x_studio_folio_opd" string="Folio OPD"/>
                    <field optional="show" name="x_studio_folio_o" string="Folio O"/>
            </field> 
        </record>
    </data>
</odoo>